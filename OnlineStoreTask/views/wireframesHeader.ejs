<!DOCTYPE html>
<html>

<head>
  <title><%= currentCat.id %></title>
  <link rel="stylesheet" href="/stylesheets/style.css" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous">
  </script>

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>

<body>
  <div class="header-wrapper"></div>
  <nav class="navbar p-0 border-bottom border-3 border-dark navbar-light bg-light">
    <div class="container d-flex justify-content-between">
      <a class="navbar-brand fs-3" href="/wireframes/">Project Title</a>
      <ul class="nav align-middle">
        <li class="nav-item text-center">
          <a class="nav-button mx-3" href="/design/products">DESIGN</a>
        </li>
        <% _.each(categories, function(category) {  %>
        <li class="nav-item text-center">
          <% if(prevDocs[0].id === category.id) { %>
          <a class="nav-button mx-3 nb-current" href="/wireframes/categories/<%= category.id %>"><%= category.id %></a>
          <% } else { %>
          <a class="nav-button mx-3" href="/wireframes/categories/<%= category.id %>"><%= category.id %></a>
          <% } %>
        </li>
        <% }); %>
      </ul>
    </div>
  </nav>
  <% if(prevDocs[0].id != 'Index Page') { %>
  <div class="container">
    <div class="row">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/wireframes/">Home</a></li>
          <% for(let i = 0; i < prevDocs.length; i++) { %>
          <% if(i != prevDocs.length - 1){ %>
          <li class="breadcrumb-item"><a href="/<%=prevDocs[i].route%>/<%=prevDocs[i].id%>"><%=prevDocs[i].name%></a>
          </li>
          <% } else { %>
          <li class="breadcrumb-item active" aria-current="page"><%=prevDocs[i].name%></li>
          <% } %>
          <% } %>
        </ol>
      </nav>
    </div>
  </div>
  <% } %>
</body>